    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit: Run main.py",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/.venv/Scripts/activate"
